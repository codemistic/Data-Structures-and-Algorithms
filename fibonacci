#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of terms: ";
    cin >> n;

    if (n <= 0) {
        cout << "Please enter a positive integer." << endl;
        return 0;
    }

    int t1 = 0, t2 = 1;
    cout << "Fibonacci Series: " << endl;

    for (int i = 1; i <= n; i++) {
        cout << t1 << " ";
        int next_term = t1 + t2;
        t1 = t2;
        t2 = next_term;
    }

    cout << endl;
    return 0;
}
